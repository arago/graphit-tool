            <xs:annotation>
              <xs:documentation>type of interface (IPv4, IPv6, VirtualIPv4, VirtualIPv6), key must be interface name
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NetworkInterfacePrefixLength" type="aae:GenericMap" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The prefix length of an IPv6 address (determines host/networ part), key must be interface name</xs:documentation>
            </xs:annotation>
            
          </xs:element>
          <xs:element name="Route" type="aae:GenericList" minOccurs="0" >
