                <xs:documentation> list of route destinations and route ips (colon separated) </xs:documentation>
              </xs:annotation>
          </xs:element>
          <xs:element name="RouteNetmask" type="aae:GenericMap" minOccurs="0">
            <xs:annotation>
              <xs:documentation>netmask address of interface (e.g. 255.255.255.0), key must be interface name
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RouteInterface" type="aae:GenericMap" minOccurs="0">
              <xs:annotation>
                <xs:documentation> inteface of route </xs:documentation>
              </xs:annotation>
          </xs:element>
          <xs:element name="RouteDestination" type="aae:GenericMap" minOccurs="0"/>
          <xs:element name="RouteGateway" type="aae:GenericMap" minOccurs="0">
