<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://graphit.co/schemas/v2/MARSSchema" 
           xmlns:aae="https://graphit.co/schemas/v2/MARSSchema"
           elementFormDefault="qualified"
           version="5.3.5" >

  <!-- Attribute Definition for reused Attributes in all Node Levels -->

  <xs:complexType name="BaseValueType">
    <xs:attribute name="Value" />
    <xs:attribute name="Key" use="optional" >
            <xs:annotation>
                <xs:documentation>
                    Key for a map entry
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    <!-- technically valid but probably of no use in mars xml
         <xs:attribute name="CreatedOn" use="optional" />
         <xs:attribute name="MTIME" use="optional" />
    -->
  </xs:complexType>

  <xs:complexType name="BaseListValueType">
    <xs:annotation>
      <xs:documentation>Basic content type for lists</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:restriction base="aae:BaseValueType">
        <xs:attribute name="Key" use="prohibited" >
            <xs:annotation>
                <xs:documentation>
                    Key for a map entry
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="BaseMapValueType">
    <xs:annotation>
      <xs:documentation>Basic content type for maps</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:restriction base="aae:BaseValueType">
        <xs:attribute name="Key" use="required"  >
            <xs:annotation>
                <xs:documentation>
                    Key for a map entry
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>

  <xs:simpleType name="AutomationStateType">
    <xs:annotation>
      <xs:documentation>special node attribute telling automation system whether to deal with it normally or not. If no value is specified the value "Automated" is assumed which means normal processing.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Automated" />
      <xs:enumeration value="Manual" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="ShortString">
    <xs:annotation>
      <xs:documentation>a name</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="100" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="LongString">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="200" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="NameString">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-zA-Z0-9].*[a-zA-Z0-9\)]" />
      <xs:maxLength value="100" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ID">
    <xs:annotation>
      <xs:documentation>an id</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="customerId">
    <xs:annotation>
        <xs:documentation>an id for a customer</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-z0-9\-\._]+" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SizeUnitType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="k" />
      <xs:enumeration value="M" />
      <xs:enumeration value="G" />
      <xs:enumeration value="T" />
      <xs:enumeration value="P" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Attribute Types -->
  <xs:simpleType name="IPv4_address">
    <xs:annotation>
      <xs:documentation>an IPv4 address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern
          value="(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IPv4_addressWithOptionalPrefix">
    <xs:annotation>
      <xs:documentation>an IPv4 address/prefix</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern
          value="(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(/[0-9]{1,2})?" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IPv6_address">
    <xs:annotation>
      <xs:documentation>an IPv6 address</xs:documentation>
    </xs:annotation>
    <!-- pattern will allow any valip IPv6 address but won't reject some invalid ones -->
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]{0,4}(:[0-9a-fA-F]{0,4}){2,7}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IPv6_prefixLength">
    <xs:annotation>
      <xs:documentation>the number of bits of an IPv6 address forming the network part </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="128"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IPv6_addressWithOptionalPrefix">
    <xs:annotation>
      <xs:documentation>IPv6 address/prefix notation</xs:documentation>
    </xs:annotation>
    <!-- pattern will allow any valip IPv6 address but won't reject some invalid ones -->
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]{0,4}(:[0-9a-fA-F]{0,4}){2,7}(/[0-9]{1,3})?" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IP">
    <xs:annotation>
      <xs:documentation>IP address</xs:documentation>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="aae:IPv4_address" />
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="aae:IPv6_address" />
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="ServiceIP">
    <xs:annotation>
      <xs:documentation>bind IP of a service. Either a valid IP or '*' (binds to all)</xs:documentation>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="aae:IP" />
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="*" />
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="RouteDestination">
    <xs:annotation>
      <xs:documentation>destination of a static IP route</xs:documentation>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="aae:IPv4_addressWithOptionalPrefix" />
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="aae:IPv6_addressWithOptionalPrefix" />
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>


  <xs:simpleType name="MAC_address">
    <xs:annotation>
      <xs:documentation>a MAC address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern
          value="[0-9a-fA-F][0-9a-fA-F][:][0-9a-fA-F][0-9a-fA-F][:][0-9a-fA-F][0-9a-fA-F][:][0-9a-fA-F][0-9a-fA-F][:][0-9a-fA-F][0-9a-fA-F][:][0-9a-fA-F][0-9a-fA-F]" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StatusType">
    <xs:annotation>
      <xs:documentation>status type of node status</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OK" />
      <xs:enumeration value="WARN" />
      <xs:enumeration value="ALARM" />
      <xs:enumeration value="CRITICAL" />
      <xs:enumeration value="MAINTENANCE" />
      <xs:enumeration value="UNKNOWN" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StatusTypeNumeric">
    <xs:annotation>
      <xs:documentation>status type of node status</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0">
        <xs:annotation>
          <xs:documentation>Numerical value for UNKNOWN</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="1">
        <xs:annotation>
          <xs:documentation>Numerical value for OK</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation>
          <xs:documentation>Numerical value for WARN</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation>
          <xs:documentation>Numerical value for CRITICAL</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ClustertypeType">
    <xs:annotation>
        <xs:documentation>Type of a cluster (Single, Loadbalanced, Cluster, Hotstandby or Coldstandby)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Single" />
      <xs:enumeration value="Loadbalanced" />
      <xs:enumeration value="Cluster" />
      <xs:enumeration value="Hotstandby" />
      <xs:enumeration value="Coldstandby" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="email">
    <xs:annotation>
      <xs:documentation>an e-mail address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern
          value="([a-zA-Z0-9_\-\.]+)@([a-zA-Z0-9_\-\.]+)\.([a-zA-Z]{2,5})" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="phone">
    <xs:annotation>
      <xs:documentation>a phone number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\+?[0-9]+" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Model Type Definitions -->
  <xs:simpleType name="aaeID">
    <xs:annotation>
      <xs:documentation>
        distinct model id (customer:project:level:FQN)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern
          value="[a-zA-Z0-9\-+_.]+:[a-zA-Z0-9\-+_.]+:(Machine|Software|Resource|Application):[a-zA-Z0-9\-+_.]*" />
    </xs:restriction>
  </xs:simpleType>

