  <xs:complexType name="Services">
    <xs:sequence>
      <xs:element name="Content" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
           <xs:documentation> basic content </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:restriction base="aae:BaseValueType">
              <xs:attribute name="Value">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[a-zA-Z0-9\-+_.]+" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="Key" use="prohibited"/>
            </xs:restriction>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

